{
  "parserOptions": {
      "sourceType": "module"
  },
  "env": {
      "es6": true
  },
  "extends": ["plugin:prettier/recommended"],
  "rules": {
      "keyword-spacing": 2,
      "no-console": "warn",
      "arrow-body-style": ["error", "always"],
       "ignoreComments": ["error"],
       "no-trailing-spaces": ["error", { "ignoreComments": false }]
  }
}